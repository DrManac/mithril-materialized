{
  "name": "example",
  "private": true,
  "version": "0.1.4",
  "description": "Example project voor the mithril-materialized library.",
  "scripts": {
    "clean": "rimraf ./.cache && rimraf ../../docs",
    "start": "parcel index.html --out-dir ../../docs",
    "build:domain": "npm run clean && parcel build index.html --out-dir ../../docs --public-url https://erikvullings.github.io/mithril-materialized && cp favicon.ico ../../docs/",
    "link": "npm link mithril-materialized",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "example",
    "mithril",
    "typescript",
    "materialize-css"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "mithril": "^2.0.0-rc.6",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mithril-materialized": "^0.13.0"
  },
  "devDependencies": {
    "@types/materialize-css": "^1.0.6",
    "@types/mithril": "^1.1.16",
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.1"
  }
}
